{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": false,
    "properties": {
        "name": {
            "enum": [
                "StatusPayload"
            ],
            "type": "string"
        },
        "status": {
            "additionalProperties": false,
            "properties": {
                "hostname": {
                    "type": "string"
                },
                "map": {
                    "type": "string"
                },
                "players": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "uptime": {
                    "type": "number"
                },
                "workshopMap": {
                    "additionalProperties": false,
                    "properties": {
                        "id": {
                            "type": "string"
                        },
                        "name": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                }
            },
            "type": "object"
        }
    },
    "type": "object"
}

