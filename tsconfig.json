{
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"@/*": ["./*"]
		},
		"target": "es2019",
		"lib": ["es2020.string", "dom"],
		"module": "commonjs",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"outDir": "./dist",
		"strictNullChecks": true
	}
}
