{
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"@/*": ["./*"]
		},
		"target": "es2022",
		"module": "commonjs",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node16",
		"outDir": "./dist",
		"strictNullChecks": true
	}
}
